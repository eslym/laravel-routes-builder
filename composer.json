    "require": {
        "php": ">=8.0",
        "illuminate/support": "^8.0",
        "illuminate/routing": "^8.0",
        "composer/composer": ">=1.0"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "eslym",
            "email": "engshun.lim@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
